{
  "manifest_version": 2,
  "name": "Cutouts: Firefox extension",
  "description": "Cut out a portion of a webpage and store inside the Cutouts bookmarker",
  "version": "1.0.2",
  "homepage_url": "https://cutouts.siddharthkannan.in",

  "icons": {
    "60": "icons/cutouts-60.png",
    "40": "icons/cutouts-40.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ]
}
